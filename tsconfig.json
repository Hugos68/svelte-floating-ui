{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node LTS",
	"_version": "20.1.0",
	"compilerOptions": {
		"outDir": "dist",
		"types": ["svelte"],
		"lib": ["DOM", "es2023"],
		"module": "node16",
		"target": "es2022",
		"strict": true,
		"declaration": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node16"
	}
}
